{
    "name": "webnuvola/excel-reader",
    "description": "Read spreadsheet files (CSV, XLSX and ODS) using OpenSpout and return data in array format",
    "keywords": [
        "excel",
        "phpexcel",
        "spreadsheet",
        "phpspreadsheet",
        "csv",
        "xls",
        "xlsx",
        "ods"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Fabio Cagliero",
            "email": "fabio@webnuvola.com"
        }
    ],
    "require": {
        "php": "~7.4.0 || ~8.0.0 || ~8.1.0",
        "cocur/slugify": "^4.0",
        "openspout/openspout": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Webnuvola\\ExcelReader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webnuvola\\ExcelReader\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --testdox"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
