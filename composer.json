{
    "name": "webnuvola/excel-reader",
    "description": "Read spreadsheet files (CSV, XLSX and ODS) using OpenSpout and return data in array format",
    "keywords": [
        "excel",
        "phpexcel",
        "spreadsheet",
        "phpspreadsheet",
        "csv",
        "xls",
        "xlsx",
        "ods"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Fabio Cagliero",
            "email": "fabio@webnuvola.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "cocur/slugify": "^4.5",
        "openspout/openspout": "^4.23"
    },
    "require-dev": {
        "pestphp/pest": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Webnuvola\\ExcelReader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webnuvola\\ExcelReader\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
